{
  "name": "nomadez/sdk-php",
  "description": "Nomadez SDK written in PHP",
  "keywords": [
    "api",
    "sdk",
    "nomadez"
  ],
  "homepage": "https://github.com/nomadez/sdk-php",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Glaser",
      "email": "andreas.c.glaser@gmail.com",
      "homepage": "http://andreas.glaser.me"
    }
  ],
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "^6.1",
    "psr/log": "^1",
    "andreas-glaser/php-helpers": "dev-master",
    "fzaninotto/faker": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^4",
    "symfony/var-dumper": "^3.0",
    "monolog/monolog": "^1.17"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-0": {
      "Nomadez\\SDK": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
